<script>

    jQuery(document).ready(function () {
        App.init();
        StepWizard.initStepWizard();
    });

    $(document).ready(function () {
        update_calc_cart();

        $('.add_tax').hide();
        $('.checkbox_tax').on('change', function () {
            if ($('.checkbox_tax').is(":checked")) {
                // it is checked
                $('.add_tax').show();
            } else {
                $('.add_tax').hide();
            }
        });

        $('.add_another').hide();
        $('input[type=radio][name=check_other]').on('change', function () {
            if (this.value == 'add_ano') {
                $('.add_another').show();
            }
            else {
                $('.add_another').hide();
            }
        });

        $('.postal_chk').on('change', function () {
            var zip = $(this).val();
//            alert(zip);
            $.ajax({
                url: 'http://localhost/heelcare/index.php/home/chk_postal/',
                data: {code: zip},
                dataType: "json",
                type: 'POST', // form submit method get/post
                success: function (data) {
                    console.log(data);
                    $("#city").autocomplete({
                        source: data
                    });
                }
            });
        });


    });

</script>

